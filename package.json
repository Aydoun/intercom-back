{
  "name": "intercom-bakend",
  "version": "1.0.0",
  "description": "Backend Service For Intercom App",
  "main": "index.js",
  "scripts": {
    "test": "export NODE_ENV=test && jest --watch",
    "test:watch": "jest --watch",
    "start": "export NODE_ENV=development && nodemon --ignore '__tests__' ./src/lib/start",
    "start:test": "export NODE_ENV=test && nodemon --ignore '__tests__' ./src/lib/start",
    "start:prd": "pm2 start ./src/lib/start.js",
    "develop": "nodemon --inspect-brk ./src/lib/start",
    "lint": "eslint . --fix",
    "precommit": "yarn lint"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "services(.*)": "<rootDir>/src/services/$1.js"
    },
    "testRegex": "\\.spec\\.js$",
    "setupFilesAfterEnv": ["<rootDir>/__tests__/shared.js"]
  },
  "keywords": [
    "Rest",
    "Node",
    "Express",
    "CRUD+"
  ],
  "author": "Mohamed Aydoun",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "faker": "^4.1.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "prettier": "1.18.2"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "express-validator": "^6.1.0",
    "fs-extra": "^8.0.1",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.3.13",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodegit": "^0.24.3",
    "nodemailer": "^6.2.1",
    "object.omit": "^3.0.0",
    "uuid": "^3.3.2"
  }
}
